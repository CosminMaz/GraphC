# GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  GraphC_config = debug
endif
ifeq ($(config),release)
  GraphC_config = release
endif

PROJECTS := GraphC

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

GraphC:
ifneq (,$(GraphC_config))
	@echo "==== Building GraphC ($(GraphC_config)) ===="
	@${MAKE} --no-print-directory -C . -f GraphC.make config=$(GraphC_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f GraphC.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   GraphC"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"